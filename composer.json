{
    "name": "nguyenvanduocit/php-git",
    "type": "library",
    "description": "PHP wrap for git command line",
    "homepage": "https://github.com/nguyenvanduocit/PHP-Git",
    "keywords": ["git", "commandline"],
    "license": "MIT",
    "authors": [
        {
            "name": "nguyenvanduocit",
            "email": "nguyenvanduocit@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"PHPGit\\": "src/PHPGit"}
    },
    "require": {
        "symfony/process": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "PHPGit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
